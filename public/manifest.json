{
  "name": "Instagram Inbox Previewer",
  "description": "Preview Instagram direct messages without opening them.",
  "version": "0.1.0",
  "manifest_version": 3,
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "permissions": ["scripting"],
  "background": {
    "service_worker": "scripts/serviceWorker.js"
  },
  "action": {
    "default_popup": "index.html"
  }
}
